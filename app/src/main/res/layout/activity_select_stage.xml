<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controlers.SelectStageActivity">

    <ImageView
        android:id="@+id/ivtc"
        android:layout_width="180dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.94"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvConter"
        app:layout_constraintVertical_bias="0.10"
        app:srcCompat="@drawable/stagetc" />

    <ImageView
        android:id="@+id/ivsv"
        android:layout_width="180dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvConter"
        app:layout_constraintVertical_bias="0.10"
        app:srcCompat="@drawable/stagesv" />

    <ImageView
        android:id="@+id/ivsvselect"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/ivsv"
        app:layout_constraintEnd_toEndOf="@+id/ivsv"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="@+id/ivsv"
        app:layout_constraintTop_toTopOf="@+id/ivsv"
        app:layout_constraintVertical_bias="0.9"
        app:srcCompat="@drawable/seleccione" />

    <ImageView
        android:id="@+id/ivtcselect"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/ivtc"
        app:layout_constraintEnd_toEndOf="@+id/ivtc"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="@+id/ivtc"
        app:layout_constraintTop_toTopOf="@+id/ivtc"
        app:layout_constraintVertical_bias="0.9"
        app:srcCompat="@drawable/seleccione" />

    <ImageView
        android:id="@+id/ivpk"
        android:layout_width="180dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvConter"
        app:layout_constraintVertical_bias="0.6"
        app:srcCompat="@drawable/stagekalos" />

    <ImageView
        android:id="@+id/ivhb"
        android:layout_width="180dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.94"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.39"
        app:srcCompat="@drawable/stagehb" />

    <ImageView
        android:id="@+id/ivsbf"
        android:layout_width="180dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.94"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.04"
        app:srcCompat="@drawable/stagesb" />

    <ImageView
        android:id="@+id/ivsbfselect"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/ivsbf"
        app:layout_constraintEnd_toEndOf="@+id/ivsbf"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="@+id/ivsbf"
        app:layout_constraintTop_toTopOf="@+id/ivsbf"
        app:layout_constraintVertical_bias="0.9"
        app:srcCompat="@drawable/seleccione" />

    <ImageView
        android:id="@+id/ivbf"
        android:layout_width="180dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.04"
        app:srcCompat="@drawable/stagebf" />

    <ImageView
        android:id="@+id/ivfd"
        android:layout_width="180dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.215"
        app:srcCompat="@drawable/stageff" />

    <ImageView
        android:id="@+id/ivps2"
        android:layout_width="180dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.39"
        app:srcCompat="@drawable/stageps2" />

    <ImageView
        android:id="@+id/ivfdselect"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/ivfd"
        app:layout_constraintEnd_toEndOf="@+id/ivfd"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="@+id/ivfd"
        app:layout_constraintTop_toTopOf="@+id/ivfd"
        app:layout_constraintVertical_bias="0.90"
        app:srcCompat="@drawable/seleccione" />

    <ImageView
        android:id="@+id/ivbfselect"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/ivbf"
        app:layout_constraintEnd_toEndOf="@+id/ivbf"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="@+id/ivbf"
        app:layout_constraintTop_toTopOf="@+id/ivbf"
        app:layout_constraintVertical_bias="0.89"
        app:srcCompat="@drawable/seleccione" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="STARTERS"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.10" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Seleccion del Ecenario"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.01999998" />

    <ImageView
        android:id="@+id/ivps2select"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/ivps2"
        app:layout_constraintEnd_toEndOf="@+id/ivps2"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="@+id/ivps2"
        app:layout_constraintTop_toTopOf="@+id/ivps2"
        app:layout_constraintVertical_bias="0.9"
        app:srcCompat="@drawable/seleccione" />

    <ImageView
        android:id="@+id/ivpkselect"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/ivpk"
        app:layout_constraintEnd_toEndOf="@+id/ivpk"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="@+id/ivpk"
        app:layout_constraintTop_toTopOf="@+id/ivpk"
        app:layout_constraintVertical_bias="0.9"
        app:srcCompat="@drawable/seleccione" />

    <TextView
        android:id="@+id/tvConter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CONTERPICKS"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivps2"
        app:layout_constraintVertical_bias="0.07" />

    <Button
        android:id="@+id/btnSStage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:enabled="false"
        android:text="Selecciona"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivpk" />

    <ImageView
        android:id="@+id/ivhbselect"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/ivhb"
        app:layout_constraintEnd_toEndOf="@+id/ivhb"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintStart_toStartOf="@+id/ivhb"
        app:layout_constraintTop_toTopOf="@+id/ivhb"
        app:layout_constraintVertical_bias="0.9"
        app:srcCompat="@drawable/seleccione" />
</androidx.constraintlayout.widget.ConstraintLayout>